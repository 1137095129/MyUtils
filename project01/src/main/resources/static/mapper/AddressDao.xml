<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.testpro.testproject.dao.AddressDao">
    <select id="selectByDeviceSerial" resultType="com.testpro.testproject.domain.Address">
        select * from address where deviceSerial=#{deviceSerial}
    </select>

    <insert id="addNewAddress">
        insert into
    </insert>

    <update id="updateToken">
        update token set token=#{token},time=#{time} where id=#{id}
    </update>
</mapper>